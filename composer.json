{
    "name": "tado/eloquent-external-storage",
    "description": "Adds transparent external storage to an eloquent model",
    "keywords": ["eloquent"],
    "license" : "MIT",
    "authors": [
        {
            "name": "Inaki Anduaga",
            "email": "inaki.anduaga@tado.com"
        }
    ],
    "support" : {
        "issues" : "https://github.com/tadodotcom/eloquent-external-storage/issues"
    },
    "require": {
        "php": ">=5.5.0",
        "illuminate/support" : "4.2.x",
        "illuminate/database": "4.2.x",
        "illuminate/console" : "4.2.x",
        "illuminate/config" : "4.2.x",
        "aws/aws-sdk-php" : "2.8.x"
    },
    "require-dev" : {
        "phpunit/phpunit": "4.3.x",
        "mockery/mockery": "1.0",
        "orchestra/testbench" : "2.2.x",
        "satooshi/php-coveralls": "2.0.0",
        "vlucas/phpdotenv" : "2.4.0"
    },
    "autoload": {
      "psr-4": {
            "Tado\\EloquentExternalStorage\\": "src/",
            "Tado\\EloquentExternalStorage\\Tests\\": "tests/src/"
        }
    },
    "minimum-stability": "stable"
}
